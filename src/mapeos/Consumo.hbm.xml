<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Consumo" table="consumo">
	    <id column="idConsumo" name="idConsumo">
	      <generator class="identity"/>
	    </id>
	    <property column="horasValle" name="horasValle" type="float"/>
	    <property column="horasPico" name="horasPico" type="float"/>
	    <property column="horasResto" name="horasResto" type="float"/>
	</class>    
</hibernate-mapping>
